/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.divisionBar{
  width: 100%;
  height: 36rpx;
  margin-top: 16px;
  background-color: #F0F0F0;
}
.chart-container{
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 14rpx 0;
  box-sizing: border-box;
  border-top: 1px solid #f0f0f0;
}
.f2-chart {
  width: 95%;
  height: 600rpx;
}
